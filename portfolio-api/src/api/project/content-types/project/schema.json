{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "shortIntro": {
      "type": "richtext"
    },
    "projectContent": {
      "type": "dynamiczone",
      "components": [
        "project-info.description",
        "project-info.files",
        "project-info.process"
      ]
    },
    "technologies": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::technology.technology",
      "mappedBy": "projects"
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "projectUrls": {
      "displayName": "Project_Urls",
      "type": "component",
      "repeatable": false,
      "component": "project-info.project-urls"
    },
    "projectCategories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::project-category.project-category",
      "inversedBy": "projects"
    },
    "inANutshell": {
      "type": "component",
      "repeatable": false,
      "component": "project-info.in-a-nutshell"
    }
  }
}
