{
  "kind": "singleType",
  "collectionName": "personal_infos",
  "info": {
    "singularName": "personal-info",
    "pluralName": "personal-infos",
    "displayName": "personalInfo",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "shortIntro": {
      "type": "richtext"
    },
    "cv": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "firstName": {
      "type": "string",
      "required": true,
      "default": "Iris Maenhout"
    },
    "contactInfo": {
      "type": "component",
      "repeatable": false,
      "component": "personal-info.contact-info"
    },
    "lastName": {
      "type": "string"
    },
    "dateOfBirth": {
      "type": "date",
      "required": true
    },
    "country": {
      "type": "string"
    },
    "InfoBulletpoints": {
      "type": "richtext",
      "required": false
    },
    "jobTitles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::job-title.job-title"
    },
    "heroImgVideo": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "knownTechnologies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::technology.technology"
    },
    "ContactSectionImgVideo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    }
  }
}
